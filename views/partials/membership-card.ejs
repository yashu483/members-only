<div class="membership-applying-card">
  <div>
    <div><img src="" alt="" /></div>
    <h2>Become a member</h2>
    <p>Answer this simple question to unlock member privileges</p>
    <p class="quest-paragraph">What is 10 + 10 ?</p>
  </div>
  <form action="/role/member" method="POST">
    <div>
      <label for="key">Your answer</label
      ><input
        type="number"
        required
        id="key"
        name="key"
        placeholder="Enter your answer"
      />
    </div>
    <button type="submit">Submit Answer</button>
  </form>
</div>
